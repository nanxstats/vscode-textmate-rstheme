{
	"name": "textmate.rstheme",
	"colors": {
		"editor.background": "#fff",
		"editor.foreground": "#000",
		"list.activeSelectionIconForeground": "#fff",
		"editorBracketHighlight.foreground1": "#687687",
		"editorBracketHighlight.foreground2": "#687687",
		"editorBracketHighlight.foreground3": "#687687",
		"editorBracketHighlight.foreground4": "#687687",
		"editorBracketHighlight.foreground5": "#687687",
		"editorBracketHighlight.foreground6": "#687687",
		"editorLineNumber.foreground": "#333333"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#4c886b"
			}
		},
		{
			"name": "Comments: Preprocessor",
			"scope": "comment.block.preprocessor",
			"settings": {
				"fontStyle": "",
				"foreground": "#4c886b"
			}
		},
		{
			"name": "Comments: Documentation",
			"scope": [
				"comment.documentation",
				"comment.block.documentation"
			],
			"settings": {
				"foreground": "#4c886b"
			}
		},
		{
			"name": "Invalid - Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator",
			"settings": {
				"foreground": "#687687"
			}
		},
		{
			"name": "Keywords",
			"scope": [
				"keyword",
				"storage",
			],
			"settings": {
				"foreground": "#687687"
			}
		},
		{
			"name": "Keywords like function, if, else, stop",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Types",
			"scope": [
				"storage.type",
				"support.type"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Language Constants",
			"scope": [
				"constant.language",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#585cf6"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"support.variable"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function",
				"support.function"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Classes",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"support.class"
			],
			"settings": {
				"foreground": "#6d79de"
			}
		},
		{
			"name": "Exceptions",
			"scope": "entity.name.exception",
			"settings": {
				"foreground": "#660000"
			}
		},
		// {
		// 	"name": "Sections",
		// 	"scope": "entity.name.section",
		// 	"settings": {
		// 		"fontStyle": "bold"
		// 	}
		// },
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant"
			],
			"settings": {
				"foreground": "#0000cd"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#036a07"
			}
		},
		{
			"name": "Strings: Escape Sequences",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#036a07"
			}
		},
		{
			"name": "Strings: Regular Expressions",
			"scope": "string.regexp",
			"settings": {
				"foreground": "#036a07"
			}
		},
		{
			"name": "Strings: Symbols",
			"scope": "constant.other.symbol",
			"settings": {
				"foreground": "#585cf6"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#687687"
			}
		},
		// HTML
		{
			"name": "HTML: Doctype Declaration",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype string",
				"meta.tag.sgml.doctype entity.name.tag",
				"meta.tag.sgml punctuation.definition.tag.html"
			],
			"settings": {
				"foreground": "#68685b"
			}
		},
		{
			"name": "HTML: Tags",
			"scope": [
				"meta.tag",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html"
			],
			"settings": {
				"foreground": "#00168e"
			}
		},
		{
			"name": "HTML: Tag Names",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#00168e"
			}
		},
		{
			"name": "HTML: Attribute Names",
			"scope": [
				"meta.tag entity.other.attribute-name",
				"entity.other.attribute-name.html"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "HTML: separator",
			"scope": "punctuation.separator.key-value.html",
			"settings": {
				"foreground": "#687687"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#585cf6"
			}
		},
		// Python
		{
			"name": "Python: separator",
			"scope": "punctuation.separator.period.python",
			"settings": {
				"foreground": "#687687"
			}
		},
		{
			"name": "Python: escape sequence",
			"scope": "constant.character.escape.python",
			"settings": {
				"foreground": "#585cf6"
			}
		},
		// CSS
		{
			"name": "CSS: Selectors",
			"scope": [
				"meta.selector",
				"meta.selector entity",
				"meta.selector entity punctuation",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#318495"
			}
		},
		{
			"name": "CSS: Property Names",
			"scope": [
				"meta.property-name",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#6d79de"
			}
		},
		{
			"name": "CSS: Property Values",
			"scope": [
				"meta.property-value",
				"meta.property-value constant.other",
				"support.constant.property-value"
			],
			"settings": {
				"foreground": "#448C27"
			}
		},
		{
			"name": "CSS: Important Keyword",
			"scope": "keyword.other.important",
			"settings": {
				"fontStyle": "bold"
			}
		},
		// JavaScript
		{
			"name": "JavaScript: function arguments",
			"scope": "variable.parameter.js",
			"settings": {
				"foreground": "#318495"
			}
		},
		{
			"name": "JavaScript: regex",
			"scope": "keyword.control.anchor.regexp",
			"settings": {
				"foreground": "#687687"
			}
		},
		{
			"name": "JavaScript: storage modifier",
			"scope": "storage.modifier.js",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		// C
		{
			"name": "C: include",
			"scope": "punctuation.definition.directive.c",
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "C: numbers",
			"scope": [
				"keyword.other.unit.hexadecimal.c",
				"keyword.other.unit.suffix.floating-point.c"
			],
			"settings": {
				"foreground": "#0000cd"
			}
		},
		// Cpp
		{
			"name": "Cpp: include, using",
			"scope": [
				"punctuation.definition.directive.cpp",
				"keyword.other.using.directive.cpp"
			],
			"settings": {
				"foreground": "#0000ff"
			}
		},
		{
			"name": "Cpp: numbers",
			"scope": "keyword.other.unit.hexadecimal.cpp",
			"settings": {
				"foreground": "#0000cd"
			}
		},
		// General Markup
		{
			"name": "Markup: Changed",
			"scope": "markup.changed",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Deletion",
			"scope": "markup.deleted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Emphasis",
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Error",
			"scope": "markup.error",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Insertion",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Markup: Link",
			"scope": "meta.link",
			"settings": {
				"foreground": "#4B83CD"
			}
		},
		{
			"name": "Markup: Output",
			"scope": [
				"markup.output",
				"markup.raw"
			],
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Prompt",
			"scope": "markup.prompt",
			"settings": {
				"foreground": "#777777"
			}
		},
		{
			"name": "Markup: Heading",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#AA3731"
			}
		},
		{
			"name": "Markup: Strong",
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Traceback",
			"scope": "markup.traceback",
			"settings": {
				"foreground": "#660000"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#036a07"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#0000cd"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#3c4c72"
			}
		},
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		// R Markdown and Markdown
		{
			"name": "YAML header entity name",
			"scope": "entity.name.tag.yaml",
			"settings": {
				"foreground": "#0000A2"
			}
		},
		{
			"name": "YAML header entity value",
			"scope": "string.unquoted.plain.out.yaml",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Code chunk options",
			"scope": "meta.embedded.block.r",
			"settings": {
				"foreground": "#3c4c72"
			}
		},
		{
			"name": "Block quote",
			"scope": "markup.quote.markdown",
			"settings": {
				"foreground": "#036a07"
			}
		},
		{
			"name": "Headings",
			"scope": [
				"punctuation.definition.heading.markdown",
				"markup.heading.markdown"
			],
			"settings": {
				"foreground": "#0c07ff"
			}
		},
		{
			"name": "Bracket links URL",
			"scope": "markup.underline.link.markdown",
			"settings": {
				"foreground": "#0c07ff"
			}
		},
		{
			"name": "Bracket links brackets",
			"scope": "punctuation.definition.link.markdown",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Link title",
			"scope": "string.other.link.title.markdown",
			"settings": {
				"foreground": "#0c07ff"
			}
		},
		{
			"name": "Link URL",
			"scope": "markup.underline.link.markdown",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"markup.bold.markdown",
				"punctuation.definition.bold.markdown"
			],
			"settings": {
				"foreground": "#0000cd"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"markup.italic.markdown",
				"punctuation.definition.italic.markdown"
			],
			"settings": {
				"foreground": "#585cf6"
			}
		},
		{
			"name": "Lists",
			"scope": [
				"markup.list",
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Inline code",
			"scope": "markup.inline.raw.string.markdown",
			"settings": {
				"foreground": "#3c4c72"
			}
		},
		{
			"name": "Raw code block",
			"scope": "markup.raw.block.markdown",
			"settings": {
				"foreground": "#3c4c72"
			}
		},
		{
			"name": "Image description",
			"scope": "string.other.link.description.markdown",
			"settings": {
				"foreground": "#0c07ff"
			}
		},
		{
			"name": "Image `!`",
			"scope": "punctuation.definition.link.description.begin.markdown",
			"settings": {
				"foreground": "#000"
			}
		},
		{
			"name": "Brackets and parenthesis in pure Markdown",
			"scope": [
				"punctuation.definition.link.title.begin.markdown",
				"punctuation.definition.link.title.end.markdown",
				"punctuation.definition.link.description.begin.markdown",
				"punctuation.definition.link.description.end.markdown",
				"punctuation.definition.metadata.markdown"
			],
			"settings": {
				"foreground": "#687687"
			}
		}
	]
}